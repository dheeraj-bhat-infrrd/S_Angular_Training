    <sql>insert user_adoption_report&#x28;company_id,company_name,region_id,region_name,branch_id,&#xd;&#xa;branch_name,invited_users,active_users,adoption_rate&#x29; &#xd;&#xa;&#x28;select tab1.COMPANY_ID COMPANY_ID,c.COMPANY COMPANY&#xd;&#xa;,tab1.REGION_ID REGION_ID,r.REGION REGION,tab1.BRANCH_ID BRANCH_ID,b.BRANCH BRANCH&#xd;&#xa;,tab1.invite_count invite_count,tab1.verified_count verified_count&#xd;&#xa;,case invite_count&#xd;&#xa;when 0 then 0&#xd;&#xa;else &#x28;&#x28;verified_count&#x2f;invite_count&#x29; &#x2a; 100&#x29; end adoption_rate&#xd;&#xa; from &#xd;&#xa;company c inner join&#xd;&#xa;&#x28;select up.COMPANY_ID,up.REGION_ID,up.BRANCH_ID,&#xd;&#xa;count&#x28;up.user_id&#x29; as invite_count,&#xd;&#xa;count&#x28;u.user_id&#x29; as verified_count&#xd;&#xa;from user_profile up left join users u&#xd;&#xa;on up.USER_ID&#x3d;u.USER_ID&#xd;&#xa; and up.COMPANY_ID &#x21;&#x3d; 1 and u.LOGIN_PASSWORD &#x3c;&#x3e; &#x27;&#x27;&#xd;&#xa; group by up.COMPANY_ID,up.REGION_ID,up.BRANCH_ID&#x29; as tab1&#xd;&#xa;on c.COMPANY_ID &#x3d; tab1.COMPANY_ID and c.COMPANY_ID &#x21;&#x3d; 1 &#xd;&#xa;left join region r on tab1.region_id&#x3d;r.region_id&#xd;&#xa;left join branch b on tab1.branch_id&#x3d;b.branch_id order by tab1.company_id&#x29;&#xd;&#xa;ON DUPLICATE KEY UPDATE company_name&#x3d;c.COMPANY,region_name&#x3d;r.REGION,branch_name&#x3d;b.BRANCH,&#xd;&#xa;invited_users&#x3d;tab1.invite_count,active_users&#x3d;tab1.verified_count,adoption_rate&#x3d;adoption_rate&#x3b;</sql>
